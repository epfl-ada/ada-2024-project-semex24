def parse_genres(x):
    if isinstance(x, str):
        try:
            return ast.literal_eval(x)
        except (ValueError, SyntaxError):
            return [genre.strip() for genre in x.split(',')]
    return x

movies_df['genres'] = movies_df['genres'].apply(parse_genres)

